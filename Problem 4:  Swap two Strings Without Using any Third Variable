def swap(str1, str2):
    str1 = str1+str2
    str2 = str1[0: (len(str1) - len(str2))]
    str1 = str1[len(str2):]
    
    print("\nString after swapping\nstr1: "+str1+"\nstr2: "+str2)



str1 = "Technical"
str2 = "Terms"
print("String before swapping\nstr1: "+str1+"\nstr2: "+str2)
swap(str1, str2)
